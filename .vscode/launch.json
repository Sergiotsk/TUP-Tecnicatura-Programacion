      "program": "d:/DEV/TUP-Tecnicatura-Programacion/UTNFRHProg2M2/C/es_archivos/consulta dos archivos/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "C/msys64/mingw64/bin/gdb.exe",
      "setupCommands": [
        {
